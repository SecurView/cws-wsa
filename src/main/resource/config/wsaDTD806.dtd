    <!--
      Element: name
      Description: The name of a log subscription.
      Format: Valid characters are letters, numbers, dash, underscore, period, space and characters `~!#$%^&()_-\{\}'\\
 
    -->
    <!ELEMENT name (#PCDATA)>
    <!--
      Element: module
      Description: The module indicates which module within the system this logfile applies to.
      Format: The module must be either 'brightmail', 'ftpd', 'gui', 'mail', 'sntpd', or 'system'.
    -->
    <!ELEMENT module (#PCDATA)>
    <!--
      Element: filename
      Description: This is the prefix used for the log filename.
      Format: Filename cannot contain spaces.
    -->
    <!ELEMENT filename (#PCDATA)>
    <!--
      Element: bytes_to_record
      Description: The number of bytes of the message to include in the bounce logs.
      Format: The value must be a number from 1 to 1,000,000.
    -->
    <!ELEMENT bytes_to_record (#PCDATA)>
    <!--
      Element: domain
      Description: This is the domain for which debug information will be logged.
      Format: The domain must be an IP address or a hostname in the following format:
- A label is a set of characters, numbers, dashes, and underscores.
- The first and last character of a label must be a letter or a number.
- The hostname must have at least 2 labels separated by a period.
- The last label cannot be all numbers.
- The IP address must be 4 numbers separated by a period. Each number must be a value from 0 to 255. (Ex: 192.168.1.1)

    -->
    <!ELEMENT domain (#PCDATA)>
    <!--
      Element: smtp_sessions
      Description: This is the number of SMTP sessions (connections) that will be recorded.
      Format: The value must be a number from 1 to 10,000.
    -->
    <!ELEMENT smtp_sessions (#PCDATA)>
    <!--
      Element: injection_ip
      Description: This is the IP, domain, or partial hostname that the system will match on for logging debug information.
      Format: The allowed hosts information must be IP fragments, addresses or ranges or hostnames, separated by commas.
    -->
    <!ELEMENT injection_ip (#PCDATA)>
    <!--
      Element: injection_sessions
      Description: This is the number of SMTP sessions (connections) that will be recorded.
      Format: The value must be a number from 1 to 10,000.
    -->
    <!ELEMENT injection_sessions (#PCDATA)>
    <!--
      Element: log_level
      Description: 
      Format: 
    -->
    <!ELEMENT log_level (#PCDATA)>
    <!ELEMENT count (#PCDATA)>
    <!ELEMENT DOMAIN (#PCDATA)>
    <!ELEMENT SRC_IP (#PCDATA)>
    <!ELEMENT DEST_IP (#PCDATA)>
    <!ELEMENT debug_modules (module*)>
    <!ELEMENT formatter_settings (filter_status_codes, log_type, w3c_fields)>
      <!--
        Element: filter_status_codes
        Description: Filter status codes validation
        Format: The HTTP Status codes should be of the form 4xx(400 - 417) and 5xx(500 - 505).
      -->
      <!ELEMENT filter_status_codes (#PCDATA)>
      <!--
        Element: log_type
        Description: This is the log type for log.
        Format: The value must be a number from 0 (W3C), 1 (Apache), 2( Squid) and 3 (Squid Details).
      -->
      <!ELEMENT log_type (#PCDATA)>
      <!--
        Element: w3c_fields
        Description: W3C logging input validation
        Format: Subscriptions should contain only the valid w3c fields.
      -->
      <!ELEMENT w3c_fields (#PCDATA)>
    <!ELEMENT retrieval (scp_push | ftp_push | ftp_poll | syslog_push)>
      <!ELEMENT scp_push (filename?, hostname, port?, login_username, directory, rollovertime?, rolloversize?, rollover?, protocol?, strict_host_checking, hostkeys?, compression?)>

         <!--
           Element: strict_host_checking
           Description: Log subscription hostkey strictness setting determines when hostkeys are used.
           Format: Value must be 'strict' or 'non-strict'
         -->
         <!ELEMENT strict_host_checking (#PCDATA)>

      <!ELEMENT ftp_push (filename?, hostname, login_username, password, directory, rollovertime?, rolloversize?, rollover?, compression?)>
      <!ELEMENT ftp_poll (filename?, rollovertime?, rolloversize?, rollover_max_files?, rollover?, alert_on_log_file_removal?, compression?)>
        <!--
          Element: login_username
          Description: This is the remote username used to authenticate to the remote server.
          Format: Whitespace characters are not allowed.
        -->
        <!ELEMENT login_username (#PCDATA)>
        <!--
          Element: directory
          Description: This is the directory on the remote server where the log files will be stored.
          Format: Valid characters in a directory path are letters, numbers, dash, underscore, slash, backslash and period.
        -->
        <!ELEMENT directory (#PCDATA)>
        <!--
          Element: rollovertime
          Description: This is the amount of time, in seconds, when the system will close the current log file and create a new one.
          Format: The rollover interval must be a time interval between 60s and 12d (60 to 1036800).  Bare numbers are interpreted as seconds, suffix numbers with 'd', 'h', or 'm' as days, hours, or minutes, respectively.
        -->
        <!ELEMENT rollovertime (#PCDATA)>
        <!--
          Element: rolloversize
          Description: This is the maximum size, in bytes, when the system will close the current log file and create a new one.
          Format: Value must be an integer from 100K to 10G.
        -->
        <!ELEMENT rolloversize (#PCDATA)>
        <!--
          Element: password
          Description: This is the password used to authenticate with the remote server.
          Format: The value cannot be '*****'.
        -->
        <!ELEMENT password (#PCDATA)>
        <!--
          Element: rollover_max_files
          Description: This is the maximum number of log files the system will keep before it starts deleting the old files.
          Format: The value must be a number from 1 to 1,000.
        -->
        <!ELEMENT rollover_max_files (#PCDATA)>
        <!--
          Element: alert_on_log_file_removal
          Description: Whether or not an alert is generated when the oldest log file is removed when log trail is full.
          Format: Value must be 1 or 0.
        -->
        <!ELEMENT alert_on_log_file_removal (#PCDATA)>
        <!ELEMENT rollover (rollover_max_files?, rolloversize?, rollovertime?, rollover_schedule?)>
          <!ELEMENT rollover_schedule (rollover_schedule_day, rollover_schedule_time)>
          <!--
            Element: rollover_schedule_day
            Description: Weekdays of the rollover schedule.
            Format: Weekdays are a comma-separated list of weekdays or weekday ranges. A weekday range is a start day, followed by a dash, followed by an end day. A weekday can be one of 'mon', 'tue', 'wed', 'thu', 'fri', 'sat', 'sun'. 
          -->
          <!ELEMENT rollover_schedule_day (#PCDATA)>
          <!--
            Element: rollover_schedule_time
            Description: Times of the rollover schedule.
            Format: Time values is a comma separated list of HH:MM-formatted times. Values cannot be duplicated. Hour is in a range 00 to 23, or the wild card "*". Minute is in a range 00 to 59, or the wild card "*". 
          -->
          <!ELEMENT rollover_schedule_time (#PCDATA)>

        <!ELEMENT compression (compression-type, compression-level)>
          <!--
            Element: compression-type
            Description: The type of compression used in compressing logs.
            Format: The value must be 'gz'
          -->
          <!ELEMENT compression-type (#PCDATA)>
          <!--
             Element: compression-level
             Description: The level of log compression used in compressing logs
             Format: The value must be 6
          -->
          <!ELEMENT compression-level (#PCDATA)>

      <!ELEMENT syslog_push (hostname, syslog_protocol, syslog_facility, compression?, syslog_buffer_enabled?, syslog_buffer_size?)>
        <!--
          Element: syslog_protocol
          Description: The protocol to use for syslog push.
          Format: The protocol must be either "UDP" or "TCP".
        -->
        <!ELEMENT syslog_protocol (#PCDATA)>
        <!--
          Element: syslog_facility
          Description: The facility to use for syslog push.
          Format: The facility must be one of: ('auth', 'authpriv', 'console', 'daemon', 'ftp', 'mail', 'ntp', 'security', 'user', 'local0', 'local1', 'local2', 'local3', 'local4', 'local5', 'local6', 'local7').
        -->
        <!ELEMENT syslog_facility (#PCDATA)>
        <!--
          Element: syslog_buffer_enabled
          Description: Enable buffering when syslog server is not reachable
          Format: The value must be Yes or No or 0 (for No) or 1 (for Yes).
        -->
        <!ELEMENT syslog_buffer_enabled (#PCDATA)>
        <!--
          Element: syslog_buffer_size
          Description: Size of the syslog temporary buffer
          Format: The value must be between 10M and 10G bytes and a multiple of 1024 bytes
        -->
        <!ELEMENT syslog_buffer_size (#PCDATA)>

<!--
  Element: log_system_measurements_frequency
  Description: This is the frequency (in seconds) of how often system measurements are logged.
  Format: A number from 1 to 100,000.
-->
<!ELEMENT log_system_measurements_frequency (#PCDATA)>

<!--
  Element: log_message_id
  Description: Whether or not to log Message-IDs.
  Format: Value must be 1 or 0.
-->
<!ELEMENT log_message_id (#PCDATA)>

