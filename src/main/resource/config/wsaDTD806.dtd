  <!ELEMENT ldap_server (ldap_server_name, ldap_server_port, ldap_server_hostname, ldap_server_pass?, ldap_server_user?, ldap_server_base, ldap_server_authtype,ldap_server_use_ssl?, ldap_server_compat?)>
    <!--
      Element: ldap_server_name
      Description: The nickname of the LDAP server.
      Format: The LDAP Server Name should be a string that can uniquely identify this server inside a particular LDAP Query.

    -->
    <!ELEMENT ldap_server_name (#PCDATA)>
    <!--
      Element: ldap_server_port
      Description: The port of the LDAP server.
      Format: The LDAP Server Port should be the TCP port number (e.g. 389) on which to connect to the LDAP server.

    -->
    <!ELEMENT ldap_server_port (#PCDATA)>
    <!--
      Element: ldap_server_hostname
      Description: The hostname of the LDAP server.
      Format: The LDAP Server Hostname should be the hostname of the LDAP server to connect to, or a comma-separated list of hostnames.

    -->
    <!ELEMENT ldap_server_hostname (#PCDATA)>
    <!--
      Element: ldap_server_pass
      Description: The password for the LDAP server.
      Format: The LDAP Server Password should be the password to use when connecting to the LDAP server.

    -->
    <!ELEMENT ldap_server_pass (#PCDATA)>
    <!--
      Element: ldap_server_user
      Description: The user for the LDAP server.
      Format: The LDAP Server User should be the username to use when connecting to the LDAP server.

    -->
    <!ELEMENT ldap_server_user (#PCDATA)>
    <!--
      Element: ldap_server_base
      Description: The base to use with the LDAP server.
      Format: The Base DN should be the LDAP-style specification of the search starting
point to use when connecting to the LDAP server. It consists of one or more
components of the form 'object=value' where 'object' is composed of only
alphanumeric characters and 'value' is composed of only the following set of
characters (alphanumeric, space, -,_,`,~,!,#,$,%,^,&,(,),\{,\},@,',.,/,:,=,<,>,[,],=,*,?,<,>,").
Multiple components are separated by a comma with or without spaces in between.
    -->
    <!ELEMENT ldap_server_base (#PCDATA)>
    <!--
      Element: ldap_server_authtype
      Description: What kind of authentication to use with the LDAP server.
      Format: The LDAP Server Authentication Type should be the type of authentication to use when connecting to the LDAP server. It is either 'anonymous' or 'password'.

    -->
    <!ELEMENT ldap_server_authtype (#PCDATA)>
    <!--
      Element: ldap_server_use_ssl
      Description: Whether or not to use SSL when connecting to the LDAP server.
      Format: The LDAP server use_ssl boolean should be zero or one specifying whether or not SSL will be used when contacting the LDAP server.
    -->
    <!ELEMENT ldap_server_use_ssl (#PCDATA)>
    <!ELEMENT ldap_server_compat (ldap_server_compat_no_attr_attr?, ldap_server_compat_scope?)>
      <!--
        Element: ldap_server_compat_no_attr_attr
        Description: Attribute to search for when no attribute is desired
        Format: Some types of LDAP Queries need parameters which specify LDAP attributes by name to look for in the objects returned by the LDAP server. The Extra Data Key and Extra Data Value parameters are used to store these additional parameters.

      -->
      <!ELEMENT ldap_server_compat_no_attr_attr (#PCDATA)>
      <!--
        Element: ldap_server_compat_scope
        Description: Value to give to LDAP server for scope setting.
        Format: Value to give to LDAP server for scope setting, can be 0, 1, or 2.
      -->
      <!ELEMENT ldap_server_compat_scope (#PCDATA)>
  <!ELEMENT ldap_query (ldap_query_name, ldap_query_server, ldap_query_type, ldap_query_query, ldap_query_ttl?, ldap_query_cache_size?, ldap_query_extra_data*)>
    <!--
      Element: ldap_query_name
      Description: The nickname of the LDAP query.
      Format: The LDAP Query Name should be a string that can uniquely identify this query inside a particular listener.

    -->
    <!ELEMENT ldap_query_name (#PCDATA)>
    <!--
      Element: ldap_query_server
      Description: The server to send the LDAP query to.
