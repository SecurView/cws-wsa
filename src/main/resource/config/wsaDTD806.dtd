    <!ELEMENT direct (jack,  jack_mtu?, noarp?)>
      <!--
        Element: jack_mtu
        Description: 
        Format: 
      -->
      <!ELEMENT jack_mtu (#PCDATA)>
      <!--
        Element: jack
        Description: 
        Format: 
      -->
      <!--
        Element: noarp
        Description: 
        Format: 
      -->
      <!ELEMENT noarp (#PCDATA)>
    <!ELEMENT loop (jack)>
      <!--
        Element: jack
        Description: 
        Format: 
      -->
      <!ELEMENT jack (#PCDATA)>
    <!ELEMENT paired ((pair_name, primary, backup, pair_mtu?) | (primary, backup, pair_mtu?))>
      <!--
        Element: pair_name
        Description: 
        Format: 
      -->
      <!ELEMENT pair_name (#PCDATA)>
      <!--
        Element: primary
        Description: 
        Format: 
      -->
      <!ELEMENT primary (#PCDATA)>
      <!--
        Element: backup
        Description: 
        Format: 
      -->
      <!ELEMENT backup (#PCDATA)>
      <!--
        Element: pair_mtu
        Description: 
        Format: 
      -->
      <!ELEMENT pair_mtu (#PCDATA)>
    <!ELEMENT vlan (tag, vlan_jack, vlan_mtu?)>
      <!--
        Element: tag
        Description: 
        Format: 
      -->
      <!ELEMENT tag (#PCDATA)>
      <!--
        Element: vlan_jack
        Description: 
        Format: 
