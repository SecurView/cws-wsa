  <!ELEMENT good_neighbor_entry (hostname, max_outbound_concurrency?, max_concurrency?, max_concurrency_virtual?, bounce_profile_name?, use_tls?, delivery_throttling?)>
    <!--
      Element: max_outbound_concurrency
      Description: The maximum concurrency for a specific host.
      Format: A number from 0 to 1,000.
    -->
    <!ELEMENT max_outbound_concurrency (#PCDATA)>
      <!--
        Attribute: limit_type
        Description: Maximum concurrency is per host or per MXIP.
        Format: The value should be 'MXIP' to indicate per-MX IP or 'host' to indicate the limit for the entire host.

        Attribute: limit_apply
        Description: Apply maximum concurrency is system wide or by Virtual Gateway(tm).
        Format: The value should be 'VG' to indicate per Virtual Gateway(tm) limit or 'system' to indicate a system-wide limit.
      -->
      <!ATTLIST max_outbound_concurrency limit_type (MXIP | host) "host"
                                         limit_apply (VG | system) "system" >

